{
  "name": "svelte-electron-template",
  "author": "Daniel Imfeld",
  "license": "MIT",
  "version": "0.0.1",
  "main": "src/electron.js",
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "NODE_ENV=development run-p start:dev autobuild",
    "start": "sirv build --single",
    "start:dev": "NODE_ENV=development sirv build --single --dev -H 0.0.0.0",
    "stories": "NODE_ENV=development start-storybook",
    "build-storybook": "build-storybook -o build",
    "static-stories": "build-storybook -c .storybook -o .out",
    "electron": "run-s build pure-electron",
    "pure-electron": "electron .",
    "electron-dev": "run-p autobuild pure-electron-dev",
    "pure-electron-dev": "NODE_ENV=development electron ."
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-commonjs": "~11.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/svelte": "^5.2.1",
    "@tailwindcss/ui": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.3.1",
    "electron": "^8.2.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.18",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.20.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.1.1",
    "tailwindcss": "^1.1.2",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3"
